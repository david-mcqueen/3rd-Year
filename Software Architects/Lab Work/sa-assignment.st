BorderedMorph subclass: #AssignmentTemp	instanceVariableNames: 'inputTemp'	classVariableNames: ''	poolDictionaries: ''	category: 'Assignment'!!AssignmentTemp commentStamp: '<historical>' prior: 0!!!AssignmentTemp methodsFor: 'calculations' stamp: 'DavidMcQueen 10/17/2014 15:46'!convertTemp		| convertedTemp |	convertedTemp:= inputTemp contents asNumber.	convertedTemp := (convertedTemp -32) / 1.8.	inputTemp contents: convertedTemp asString.! !!AssignmentTemp methodsFor: 'initialization' stamp: 'DavidMcQueen 10/20/2014 10:05'!initialize	| titleLabel convertButton |	super initialize.		self borderWidth: 2.	bounds := (152@48.0 corner: 583@328.0).	self color: Color lightGray.			titleLabel := StringMorph new.	titleLabel contents: 'Fahrenheit to Celsius converter'.	titleLabel bounds: (278.0@74.0 corner: 492.0@87.0).	self addMorph: titleLabel.		convertButton := SimpleButtonMorph new.	convertButton bounds: (298.0@249.0 corner: 423.0@286.0).	convertButton target: self.	convertButton actionSelector: #convertTemp.	convertButton label: 'Convert'.	self addMorph: convertButton.		inputTemp := TextMorph new.	inputTemp backgroundColor: Color white.	inputTemp bounds: (240.0@93.0 corner: 512.0@107.0).	self addMorph: inputTemp.	! !